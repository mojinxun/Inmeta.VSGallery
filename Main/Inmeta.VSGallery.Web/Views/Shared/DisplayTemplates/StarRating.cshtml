@model Inmeta.VSGallery.Web.Models.StarRating


<span class="Rating">
    @for (int i = 1; i <= 5; i++)
    {
        <input name="@Model.ReleaseId" type="radio" class="star" disabled="disabled" 
        @if (Model.NrStars == i){
            @:checked="checked" 
        } />   
    }
</span>